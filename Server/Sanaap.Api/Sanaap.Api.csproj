<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="System.Data.Entity.Internal.EdmMetadataContext.edmx" />
    <None Remove="System.Data.Entity.Migrations.History.LegacyHistoryContext.edmx" />
    <None Remove="System.Data.Entity.Migrations.Utilities.EmptyContext.edmx" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Bit.Data.EntityFramework" Version="0.9.2" />
    <PackageReference Include="Bit.IdentityServer" Version="0.9.2" />
    <PackageReference Include="Bit.OData" Version="0.9.2" />
    <PackageReference Include="Bit.OwinCore" Version="0.9.2" />
    <PackageReference Include="Bit.OwinCore.AspNetCore2Upgrade" Version="0.9.2" />
    <PackageReference Include="BitCodeAnalyzer" Version="1.1.9.8" />
    <PackageReference Include="Microsoft.AspNetCore" Version="2.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Sanaap.Dto\Sanaap.Dto.csproj" />
    <ProjectReference Include="..\Sanaap.Model\Sanaap.Model.csproj" />
    <ProjectReference Include="..\Sanaap.Service\Sanaap.Service.csproj" />
    <ProjectReference Include="..\Sannap.Data\Sannap.Data.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Content Update="environments.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Update="IdentityServerCertificate.pfx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
